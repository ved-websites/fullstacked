{
  "name": "api",
  "version": "0.0.0",
  "description": "NestJS API implementing GraphQL and Prisma with auto generated queries.",
  "private": true,
  "engines": {
    "node": "^18.0.0",
    "npm": "^8.1.0"
  },
  "scripts": {
    "init": "gulp init",
    "build": "gulp build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:no-watch": "nest start --debug",
    "start:prod": "node ./dist/main.js",
    "clean:db": "gulp cleanDb",
    "delete:dist": "gulp deleteDist",
    "generate": "gulp setupPrisma",
    "gql:generate": "gulp setupGQLSchema",
    "gql:generate:prismaless": "gulp generateGraphQLSchema",
    "lint": "npm-run-all -p lint:*",
    "lint:src": "eslint \"{src,apps,libs}/**/*.ts\"",
    "lint:tests": "eslint \"tests/**/*.ts\"",
    "format": "npm-run-all format:*",
    "format:prettier": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:eslint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "format:prisma": "prisma format",
    "test": "vitest --config tests/.vitest/vitest.config.all.ts",
    "test:specs": "vitest --config tests/.vitest/vitest.config.specs.ts",
    "test:e2e": "vitest --config tests/.vitest/vitest.config.e2e.ts",
    "coverage": "pnpm run test --coverage",
    "seed": "gulp seed",
    "studio": "prisma studio"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "4.9.3",
    "@lucia-auth/adapter-prisma": "3.0.1",
    "@nestjs/apollo": "12.0.9",
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.2.5",
    "@nestjs/core": "10.2.5",
    "@nestjs/event-emitter": "2.0.2",
    "@nestjs/graphql": "12.0.9",
    "@nestjs/platform-express": "10.2.5",
    "@nestjs/throttler": "5.0.0",
    "@paljs/plugins": "5.3.3",
    "@prisma/client": "4.16.2",
    "axios": "1.5.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.6",
    "dotenv": "16.3.1",
    "dotenv-expand": "10.0.0",
    "express-handlebars": "7.1.2",
    "graphql": "16.8.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-upload": "15.0.2",
    "graphql-ws": "5.14.0",
    "lucia": "2.6.0",
    "nest-typed-config": "2.8.0",
    "nestjs-i18n": "10.3.5",
    "nestjs-minio-client": "2.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@nestjs/cli": "10.1.17",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.5",
    "@swc/core": "1.3.86",
    "@types/cookie-parser": "1.4.4",
    "@types/express": "4.17.17",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/graphql-upload": "15.0.2",
    "@types/gulp": "4.0.13",
    "@types/minio": "7.1.0",
    "@types/supertest": "2.0.12",
    "@v-ed/tsconfig": "0.3.0",
    "@vitest/coverage-v8": "0.34.4",
    "csstype": "3.1.2",
    "del": "6.1.1",
    "graphql-config": "5.0.2",
    "graphql-tag": "2.12.6",
    "gulp": "4.0.2",
    "prisma": "4.16.2",
    "prisma-fixtures": "0.1.15",
    "prisma-nestjs-graphql": "18.0.2",
    "supertest": "6.3.3",
    "supertest-graphql": "1.1.4",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "unplugin-swc": "1.4.2",
    "vitest": "0.34.4"
  }
}
