{
  "name": "api",
  "version": "0.0.0",
  "description": "NestJS API implementing GraphQL and Prisma with auto generated queries.",
  "private": true,
  "engines": {
    "node": "^16.0.0",
    "npm": "^8.1.0"
  },
  "scripts": {
    "init": "gulp init",
    "build": "gulp build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:no-watch": "nest start --debug",
    "start:prod": "node ./dist/main.js",
    "clean:db": "gulp cleanDb",
    "delete:dist": "gulp deleteDist",
    "generate": "gulp setupPrisma",
    "lint": "npm-run-all lint:*",
    "lint:src": "eslint \"{src,apps,libs}/**/*.ts\"",
    "lint:tests": "eslint \"tests/**/*.ts\"",
    "format": "npm-run-all format:*",
    "format:prettier": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:eslint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "format:prisma": "prisma format",
    "test": "vitest --config tests/.vitest/vitest.all-config.ts",
    "test:specs": "vitest --config tests/.vitest/vitest.specs-config.ts",
    "test:e2e": "vitest --config tests/.vitest/vitest.e2e-config.ts",
    "test:cov": "pnpm test --coverage",
    "seed": "gulp seed",
    "studio": "prisma studio"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/apollo": "10.0.19",
    "@nestjs/common": "9.0.8",
    "@nestjs/core": "9.0.8",
    "@nestjs/graphql": "10.0.21",
    "@nestjs/platform-express": "9.0.8",
    "@paljs/plugins": "4.1.0",
    "@prisma/client": "4.1.1",
    "@prisma/internals": "4.1.1",
    "apollo-server-core": "3.10.3",
    "apollo-server-express": "3.10.3",
    "axios": "0.27.2",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.2",
    "deepmerge": "4.2.2",
    "dotenv": "16.0.1",
    "dotenv-expand": "8.0.3",
    "graphql": "16.3.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.9.1",
    "nest-typed-config": "2.4.6",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.7"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@nestjs/cli": "9.1.8",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.8",
    "@swc/core": "1.3.27",
    "@types/express": "4.17.15",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/gulp": "4.0.10",
    "@types/gulp-rename": "2.0.1",
    "@types/supertest": "2.0.12",
    "@v-ed/tsconfig": "0.2.0",
    "c8": "7.12.0",
    "del": "6.1.1",
    "graphql-config": "4.3.6",
    "graphql-tag": "2.12.6",
    "gulp": "4.0.2",
    "gulp-prompt": "1.2.0",
    "gulp-rename": "2.0.0",
    "gulp-replace": "1.1.4",
    "gulp-typescript": "6.0.0-alpha.1",
    "prisma": "4.1.1",
    "prisma-fixtures": "0.1.11",
    "prisma-nestjs-graphql": "17.0.2",
    "supertest": "6.2.4",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "unplugin-swc": "1.3.2",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.20.3"
  }
}
