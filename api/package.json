{
  "name": "api",
  "version": "0.0.0",
  "description": "NestJS API implementing GraphQL and Prisma with auto generated queries.",
  "private": true,
  "engines": {
    "node": "^18.0.0",
    "npm": "^8.1.0"
  },
  "scripts": {
    "init": "gulp init",
    "build": "gulp build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:no-watch": "nest start --debug",
    "start:prod": "node ./dist/main.js",
    "clean:db": "gulp cleanDb",
    "delete:dist": "gulp deleteDist",
    "generate": "gulp setupPrisma",
    "lint": "npm-run-all -p lint:*",
    "lint:src": "eslint \"{src,apps,libs}/**/*.ts\"",
    "lint:tests": "eslint \"tests/**/*.ts\"",
    "format": "npm-run-all format:*",
    "format:prettier": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:eslint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "format:prisma": "prisma format",
    "test": "vitest --config tests/.vitest/vitest.config.all.ts",
    "test:specs": "vitest --config tests/.vitest/vitest.config.specs.ts",
    "test:e2e": "vitest --config tests/.vitest/vitest.config.e2e.ts",
    "coverage": "pnpm run test --coverage",
    "seed": "gulp seed",
    "studio": "prisma studio"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "4.7.4",
    "@lucia-auth/adapter-prisma": "3.0.0-beta.1",
    "@nestjs/apollo": "11.0.6",
    "@nestjs/common": "9.4.2",
    "@nestjs/core": "9.4.2",
    "@nestjs/graphql": "11.0.6",
    "@nestjs/platform-express": "9.4.2",
    "@paljs/plugins": "5.3.3",
    "@prisma/client": "4.15.0",
    "axios": "1.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.6",
    "deepmerge": "4.3.1",
    "dotenv": "16.3.0",
    "dotenv-expand": "10.0.0",
    "graphql": "16.6.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.13.1",
    "lucia": "2.0.0-beta.2",
    "nest-typed-config": "2.5.2",
    "nestjs-minio-client": "2.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "tslib": "2.5.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.2",
    "@swc/core": "1.3.61",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/gulp": "4.0.11",
    "@types/minio": "7.1.0",
    "@types/supertest": "2.0.12",
    "@v-ed/tsconfig": "0.3.0",
    "@vitest/coverage-v8": "0.32.2",
    "del": "6.1.1",
    "graphql-config": "5.0.2",
    "graphql-tag": "2.12.6",
    "gulp": "4.0.2",
    "gulp-typescript": "6.0.0-alpha.1",
    "prisma": "4.15.0",
    "prisma-fixtures": "0.1.15",
    "prisma-nestjs-graphql": "18.0.2",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "unplugin-swc": "1.3.2",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.32.2"
  }
}
