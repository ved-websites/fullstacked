{
  "name": "@app/api",
  "version": "0.1.0",
  "description": "NestJS API implementing TS-Rest and Prisma with custom realtime support.",
  "private": true,
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^10.0.0"
  },
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "init": "gulp init",
    "init:no-db-push": "gulp initNoDbPush",
    "build": "gulp build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:no-watch": "nest start --debug",
    "start:prod": "node ./dist/main.js",
    "clean:db": "gulp cleanDb",
    "delete:dist": "gulp deleteDist",
    "generate": "gulp setupPrisma",
    "lint": "npm-run-all -p lint:*",
    "lint:src": "eslint \"{src,apps,libs}/**/*.ts\"",
    "lint:tests": "eslint \"tests/**/*.ts\"",
    "format": "npm-run-all format:*",
    "format:prettier": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:eslint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "format:prisma": "prisma-case-format -f prisma/schema.prisma --map-field-case=snake --map-table-case=snake,plural -p",
    "test": "vitest --config tests/.vitest/vitest.config.all.mts",
    "test:specs": "vitest --config tests/.vitest/vitest.config.specs.mts",
    "test:e2e": "vitest --config tests/.vitest/vitest.config.e2e.mts",
    "coverage": "pnpm run test --coverage",
    "seed": "gulp seed",
    "studio": "prisma studio",
    "sentry:sourcemaps": "npm-run-all -s sentry:sourcemaps:inject sentry:sourcemaps:upload",
    "sentry:sourcemaps:inject": "sentry-cli sourcemaps inject --org v-ed --project fullstacked ./dist",
    "sentry:sourcemaps:upload": "sentry-cli sourcemaps upload --org v-ed --project fullstacked ./dist"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/axios": "4.0.0",
    "@nestjs/common": "11.1.3",
    "@nestjs/core": "11.1.3",
    "@nestjs/event-emitter": "3.0.1",
    "@nestjs/platform-express": "11.1.3",
    "@nestjs/platform-ws": "11.1.3",
    "@nestjs/schedule": "6.0.0",
    "@nestjs/throttler": "6.4.0",
    "@nestjs/websockets": "11.1.3",
    "@node-rs/argon2": "2.0.2",
    "@prisma/adapter-pg": "6.11.1",
    "@prisma/client": "6.11.1",
    "@sentry/nestjs": "9.34.0",
    "@sentry/profiling-node": "9.34.0",
    "@sveltekit-i18n/parser-default": "1.1.1",
    "@ts-rest/core": "3.53.0-rc.1",
    "@ts-rest/nest": "3.53.0-rc.1",
    "axios": "1.10.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cookie-parser": "1.4.7",
    "dotenv": "17.0.1",
    "dotenv-expand": "12.0.2",
    "express-handlebars": "8.0.3",
    "nest-typed-config": "2.10.1",
    "nestjs-i18n": "10.5.1",
    "nestjs-minio-client": "2.2.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "ws": "8.18.3",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@faker-js/faker": "9.9.0",
    "@nestjs/cli": "11.0.7",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.1.3",
    "@sentry/cli": "2.46.0",
    "@swc/core": "1.12.9",
    "@types/cookie-parser": "1.4.9",
    "@types/express": "5.0.3",
    "@types/gulp": "4.0.17",
    "@types/multer": "2.0.0",
    "@types/supertest": "6.0.3",
    "@types/ws": "8.18.1",
    "@v-ed/tsconfig": "0.4.0",
    "@vitest/coverage-v8": "3.2.4",
    "csstype": "3.1.3",
    "del": "6.1.1",
    "gulp": "5.0.1",
    "prisma": "6.11.1",
    "prisma-case-format": "2.2.1",
    "prisma-fixtures": "0.1.15",
    "replace-in-file": "7.1.0",
    "supertest": "7.1.1",
    "ts-node": "10.9.2",
    "tsx": "4.20.3",
    "typescript": "5.9.2",
    "unplugin-swc": "1.5.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "zod-prisma-types": "3.2.4"
  }
}
