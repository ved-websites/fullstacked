{
  "name": "@app/api",
  "version": "0.0.0",
  "description": "NestJS API implementing TS-Rest and Prisma with custom realtime support.",
  "private": true,
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^9.0.0"
  },
  "packageManager": "pnpm@9.15.1",
  "scripts": {
    "init": "gulp init",
    "init:no-db-push": "gulp initNoDbPush",
    "build": "gulp build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:no-watch": "nest start --debug",
    "start:prod": "node ./dist/main.js",
    "clean:db": "gulp cleanDb",
    "delete:dist": "gulp deleteDist",
    "generate": "gulp setupPrisma",
    "lint": "npm-run-all -p lint:*",
    "lint:src": "eslint \"{src,apps,libs}/**/*.ts\"",
    "lint:tests": "eslint \"tests/**/*.ts\"",
    "format": "npm-run-all format:*",
    "format:prettier": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:eslint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "format:prisma": "prisma-case-format -f prisma/schema.prisma --map-field-case=snake --map-table-case=snake,plural -p",
    "test": "vitest --config tests/.vitest/vitest.config.all.mts",
    "test:specs": "vitest --config tests/.vitest/vitest.config.specs.mts",
    "test:e2e": "vitest --config tests/.vitest/vitest.config.e2e.mts",
    "coverage": "pnpm run test --coverage",
    "seed": "gulp seed",
    "studio": "prisma studio",
    "sentry:sourcemaps": "npm-run-all -s sentry:sourcemaps:inject sentry:sourcemaps:upload",
    "sentry:sourcemaps:inject": "sentry-cli sourcemaps inject --org v-ed --project fullstacked ./dist",
    "sentry:sourcemaps:upload": "sentry-cli sourcemaps upload --org v-ed --project fullstacked ./dist"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@lucia-auth/adapter-prisma": "4.0.1",
    "@nestjs/axios": "4.0.0",
    "@nestjs/common": "11.0.8",
    "@nestjs/core": "11.0.8",
    "@nestjs/event-emitter": "3.0.0",
    "@nestjs/platform-express": "11.0.8",
    "@nestjs/platform-ws": "11.0.8",
    "@nestjs/throttler": "6.4.0",
    "@nestjs/websockets": "11.0.8",
    "@prisma/client": "6.3.1",
    "@sentry/nestjs": "8.54.0",
    "@sentry/profiling-node": "8.54.0",
    "@sveltekit-i18n/parser-default": "1.1.1",
    "@ts-rest/core": "3.51.0",
    "@ts-rest/nest": "3.51.0",
    "axios": "1.7.9",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.7",
    "dotenv": "16.4.7",
    "dotenv-expand": "12.0.1",
    "express-handlebars": "8.0.1",
    "lucia": "3.2.2",
    "nest-typed-config": "2.9.3",
    "nestjs-i18n": "10.5.0",
    "nestjs-minio-client": "2.2.0",
    "oslo": "1.2.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "tslib": "2.8.1",
    "ws": "8.18.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "9.4.0",
    "@nestjs/cli": "11.0.2",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.8",
    "@sentry/cli": "2.41.1",
    "@swc/core": "1.10.14",
    "@types/cookie-parser": "1.4.8",
    "@types/express": "5.0.0",
    "@types/gulp": "4.0.17",
    "@types/multer": "1.4.12",
    "@types/supertest": "6.0.2",
    "@types/ws": "8.5.14",
    "@v-ed/tsconfig": "0.4.0",
    "@vitest/coverage-v8": "3.0.5",
    "csstype": "3.1.3",
    "del": "6.1.1",
    "gulp": "5.0.0",
    "prisma": "6.3.1",
    "prisma-case-format": "2.2.1",
    "prisma-fixtures": "0.1.15",
    "supertest": "7.0.0",
    "ts-node": "10.9.2",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "unplugin-swc": "1.5.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.5",
    "zod-prisma-types": "3.2.3"
  }
}
